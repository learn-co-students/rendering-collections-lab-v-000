<table>
  <tr>
    <th>Order Id</th>
    <th>Product
    <th>Price
    <th>Customer</th>  
  </tr>
  <% @orders.products.each do |product| %>
    <tr>
      <td><%= @orders.id %></td>
      <td><%= product.name %></td>
      <td><%= product.price %></td>
      <td><%= @orders.customer.name %></td>
    </tr>
  <% end %>
</table>